<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rekin_biznesu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Rekin biznesu"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/RekinTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Porownawczy"></activity>
        <activity android:name=".Kalkulacyjny"></activity>
        <activity android:name=".Baza"></activity>
        <activity android:name=".Rezultaty"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>